{
  "port": 3200,
  "name": "MyBot",
  "workspace": "/path/to/openclaw/workspace",
  "openclawHome": "/path/to/.openclaw",
  "cache": { "ttlMs": 10000 },
  "model": "claude-sonnet-4-20250514",
  "skillDirs": [
    "/path/to/openclaw/skills",
    "/path/to/workspace/skills"
  ],
  "services": [
    {
      "name": "example-http",
      "type": "http",
      "url": "http://localhost:8080",
      "healthPath": "/health"
    },
    {
      "name": "example-command",
      "type": "command",
      "command": "gh auth status",
      "timeout": 5000,
      "label": "myaccount"
    },
    {
      "name": "example-file",
      "type": "file-exists",
      "path": "/path/to/check/",
      "label": "user@example.com"
    }
  ],
  "email": [
    {
      "name": "primary",
      "address": "bot@example.com",
      "provider": "himalaya",
      "command": "himalaya envelope list --folder INBOX 2>/dev/null | grep '\\*' | wc -l"
    }
  ],
  "docker": {
    "provider": "portainer",
    "url": "https://localhost:9443",
    "token": "YOUR_PORTAINER_TOKEN",
    "endpointId": 1,
    "containerFilter": "mybot"
  },
  "devServers": {
    "processGrep": "next dev",
    "projectBasePath": "/path/to/projects/"
  }
}
